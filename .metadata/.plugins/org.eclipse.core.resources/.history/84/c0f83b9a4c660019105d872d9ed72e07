package mult.thread;

public class Test {
	
}
class Thread1 extends Thread{
	public void run(){
		for(int i=0;i<10;i++) {
			System.out.println("currentThread:"+Thread.currentThread()+i);
		}
	}
}
class Thread2 extends Thread{
	public void run(){
		for(int i=0;i<10;i++) {
			System.out.println("currentThread:"+Thread.currentThread()+i);
		}
	}
}

